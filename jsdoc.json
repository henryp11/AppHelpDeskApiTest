{
  "plugins": [],
  "source": {
    "include": ["api"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules|docs)"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "opts": {
    "recurse": "true",
    "destination": "./docs",
    "template": "node_modules/docdash"
  }
}
